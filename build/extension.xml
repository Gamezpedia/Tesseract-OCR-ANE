<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.lj.ane.tesseract</id>
    <versionNumber>0.0.1</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libTesseractANE.a</nativeLibrary>
                <initializer>LJTesseractANEInitializer</initializer>
                <finalizer>LJTesseractANEFinalizer</finalizer>
            </applicationDeployment>
        </platform>
		<platform name="Android-ARM"> 
            <applicationDeployment> 
                <nativeLibrary>classes.jar</nativeLibrary> 
                <initializer>com.lj.ane.tesseract.TesseractANE</initializer> 
            </applicationDeployment> 
        </platform>
		<platform name="default">
			<applicationDeployment/>
		</platform>
    </platforms>
</extension>